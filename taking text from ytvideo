      "source": [
        "from youtube_transcript_api import YouTubeTranscriptApi\n",
        "\n",
        "videoid=input(\"pls enter the video url: \")\n",
        "\n",
        "try:\n",
        "  transcript = YouTubeTranscriptApi.get_transcript(videoid)\n",
        "\n",
        "  for line in transcript:\n",
        "    print(line[\"text\"])\n",
        "\n",
        "except Exception as e:\n",
        "    print(\"Error:\", e)\n",
        "\n",
        "\n",
        "\n"
      ]
